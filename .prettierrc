  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^next",
    "^react",
    "^@react/(.*)$",
    "^@config/(.*)$",
    "^@lib/(.*)$",
    "^@app/(.*)$",
    "^@stores/(.*)$",
    "^@features/(.*)$",
    "^@providers/(.*)$",
    "^@services/(.*)$",
    "^@components/(.*)$",
    "^@interfaces/(.*)$",
    "^@hooks/(.*)$",
